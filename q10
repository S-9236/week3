#include <stdio.h>
int main() {
    int m, n, i, j, count = 0;
    printf("enter number of rows: ");
    scanf("%d", &m);
    printf("enter number of columns: ");
    scanf("%d", &n);
    int arr[m][n];
    printf("enter elements of the %dx%d array:\n", m, n);
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &arr[i][j]);
            if (arr[i][j] != 0) {
                count++;
            }
        }
    }
    printf("total non-zero elements = %d\n", count);
    return 0;
}
